<% @page_title = "Update Admin User"%>
<%= link_to("<< Back to admin list", admin_users_path, :class=> 'back-link') %>

<div class="edit">
<h2>Update Admin User</h2>
    <%= form_for(@adminuser) do |f| %>
        <%= render(:partial=> 'form', :locals => {:f => f}) %>

        <div class="form-buttons">
            <%= f.submit("Update User")%>
        </div>
    <% end %>
</div>